<tbody id="tbody-singles">

  <% singles.each do |music| %>

    <tr>
      <th scope="row">
        <div class="media align-items-center">
          <span class="badge badge-dot mx-3">
            <i class="fas fa-music"></i>
          </span>
          <div class="media-body">
            <span class="mb-0 text-sm">
              <%= music.data[:title] %>
            </span>
          </div>
        </div>
      </th>
      <td>
        <span class="badge badge-dot mr-4">
          <i class="bg-warning text-sm"></i>&nbsp;<%= load_music_status(music.data[:status]) %>
        </span>
      </td>
      <td>
        <div class="media align-items-center">
          <div class="media-body text-center">
            <span class="mb-0 text-sm">
              <%= music.data[:message] %>
            </span>
          </div>
        </div>
      </td>
    </tr>
  <% end %>
</tbody>
